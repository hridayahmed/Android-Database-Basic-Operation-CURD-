<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hridha.Database_Basic_Operation">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.hridha.Database_Basic_Operation.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.hridha.Database_Basic_Operation.Register" />
        <activity android:name="com.hridha.Database_Basic_Operation.Welcome" />
        <activity android:name="com.hridha.Database_Basic_Operation.ShowProfile" />
        <activity android:name="com.hridha.Database_Basic_Operation.YourProfile" />
        <activity android:name="com.hridha.Database_Basic_Operation.ViewAllEmployee" />
        <activity android:name="com.hridha.Database_Basic_Operation.ViewEmployee"></activity>
    </application>

</manifest>